{
  "name": "astro-flat-file-cms",
  "version": "1.0.0",
  "description": "Astro blog instances relying on an flat-file CMS Astro integration",
  "workspaces": [
    "examples/michaels-astro-blog",
    "packages/*"
  ],
  "keywords": [
    "Astro"
  ],
  "scripts": {
    "format": "npx prettier --plugin=prettier-plugin-astro -w ./**/src/**/{*.ts,*.astro}"
  },
  "author": "Christian Kozalla",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.7.0"
  }
}
{
  "name": "flat-file-cms",
  "version": "0.1.0",
  "description": "An Astro Integration to provide a Flat-File CMS",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./utils": "./dist/blog-backend/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christiankozalla/astro-flat-file-cms.git",
    "directory": "packages/flat-file-cms/"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "rm -rf dist && tsc && npm run copy-assets",
    "copy-assets": "cp -r src/. dist/ && rm dist/index.ts"
  },
  "author": "Christian Kozalla",
  "license": "MIT",
  "devDependencies": {
    "@types/marked": "^4.0.7",
    "vite": "^3.2.3"
  },
  "peerDependencies": {
    "astro": "^1.6.3",
    "marked": "^4.2.2"
  }
}